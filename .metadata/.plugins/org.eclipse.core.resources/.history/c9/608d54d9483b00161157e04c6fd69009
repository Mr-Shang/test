package BiTree;

public class BiTreeNode {
	public static Object data;//结点的数据
	public BiTreeNode lchild,rchild;//结点的左、右孩子域
	//构造一个空结点
	public BiTreeNode(){
		this(null);
	}
	public BiTreeNode(Object object){
		this(data,null,null);
	}
	public BiTreeNode(Object data, BiTreeNode lchild, BiTreeNode rchild) {
		this.data = data;
		this.lchild = lchild;
		this.rchild = rchild;
	}
	

}
